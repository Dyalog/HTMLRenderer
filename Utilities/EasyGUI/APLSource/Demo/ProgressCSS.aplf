 r←'body{background-color:#121413;color:#117241}.radio-btn{	position:absolute;	opacity:0;	visibility:hidden;	&:checked+.label{		color:$accent-color;		&::before{			margin-right:2rem;			border:0.5remsolid$accent-color;			background:$primary-color;		}	}}progress{border-radius:2px;width:100%;height:22px;}progress::-webkit-progress-bar{background-color:#435448;border-radius:2px;}progress::-webkit-progress-value{border-radius:2px;}h3,h4{  text-align: center;  padding: 30px 0px 0px 0px;  font: 25px Oswald;  color:#117241;  text-transform: uppercase;  text-shadow: #000 0px 1px 5px;  margin: 0px;}}'
